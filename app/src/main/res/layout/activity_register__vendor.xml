<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f7ffdd"
    tools:context=".Register_Driver">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#f7ffdd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".Register_Driver">

        <EditText
            android:id="@+id/dri_city"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_marginEnd="156dp"
            android:layout_marginRight="156dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="City"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/dri_state"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dri_street_address" />

        <EditText
            android:id="@+id/dri_zip_code"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="Zip"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/dri_username"
            app:layout_constraintEnd_toEndOf="@+id/dri_username"
            app:layout_constraintTop_toBottomOf="@+id/dri_state" />


        <EditText
            android:id="@+id/dri_first_name"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_marginTop="232dp"
            android:layout_marginBottom="53dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="First Name"
            android:inputType="textPersonName"
            android:textColor="#707070"
            app:layout_constraintBottom_toTopOf="@+id/dri_id"
            app:layout_constraintEnd_toEndOf="@+id/dri_id"
            app:layout_constraintTop_toTopOf="parent" />



        <EditText
            android:id="@+id/dri_password"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="83dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="Password"
            android:inputType="textPersonName"
            android:textColor="#707070"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/dri_city"
            app:layout_constraintStart_toStartOf="@+id/dri_city"
            app:layout_constraintTop_toBottomOf="@+id/dri_username" />

        <EditText
            android:id="@+id/dri_username"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="Username"
            android:inputType="textPersonName"
            app:layout_constraintBottom_toTopOf="@+id/dri_password"
            app:layout_constraintEnd_toEndOf="@+id/dri_password"
            app:layout_constraintStart_toStartOf="@+id/dri_password"
            app:layout_constraintTop_toBottomOf="@+id/dri_zip_code" />

        <EditText
            android:id="@+id/dri_street_address"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="Address"
            android:inputType="textPersonName"
            android:textColor="#707070"
            app:layout_constraintBottom_toTopOf="@+id/dri_city"
            app:layout_constraintEnd_toEndOf="@+id/dri_city"
            app:layout_constraintTop_toBottomOf="@+id/dri_id" />

        <EditText
            android:id="@+id/dri_state"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_marginBottom="1dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="State"
            android:inputType="textPersonName"
            android:textColor="#707070"
            app:layout_constraintBottom_toTopOf="@+id/dri_zip_code"
            app:layout_constraintEnd_toEndOf="@+id/dri_zip_code"
            app:layout_constraintTop_toBottomOf="@+id/dri_city" />

        <EditText
            android:id="@+id/dri_id"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_marginBottom="2dp"
            android:background="#FFFFFF"
            android:ems="10"
            android:hint="Driver's liscence"
            android:inputType="textPersonName"
            android:textColor="#707070"
            app:layout_constraintBottom_toTopOf="@+id/dri_street_address"
            app:layout_constraintEnd_toEndOf="@+id/dri_street_address"
            app:layout_constraintTop_toBottomOf="@+id/dri_first_name" />

        <ImageView
            android:id="@+id/phone_icon"
            android:layout_width="185dp"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_marginTop="61dp"
            android:layout_marginBottom="88dp"
            android:contentDescription="TODO"
            android:src="@drawable/gotit_logo"
            app:layout_constraintBottom_toTopOf="@+id/dri_first_name"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/submit4_button"
        android:layout_width="145dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:background="#B1CF5F"
        android:text="Submit"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>